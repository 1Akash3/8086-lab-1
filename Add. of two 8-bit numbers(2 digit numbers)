section .data
    lower_byte db 0
    higher_byte db 0

section .text
    global _start

_start:
    mov al, 12
    mov bl, 25
    add al, bl
    aam
    add al, '0'
    mov [lower_byte], al
    add ah, '0'
    mov [higher_byte], ah

    mov eax, 4
    mov ebx, 1
    mov ecx, higher_byte
    mov edx, 1
    int 80h

    mov eax, 4
    mov ebx, 1
    mov ecx, lower_byte
    mov edx, 1
    int 80h

    mov eax, 1
    int 80h
