section .data
    msg db "The subtraction of two 8-bit nos is", 10, 13
    len1 equ $ - msg

section .bss
    res resb 1

section .text
    global _start

_start:
    mov al, 6
    mov bl, 5
    sub al, bl
    add al, '0'
    mov [res], al

    mov eax, 4
    mov ebx, 1
    mov ecx, msg
    mov edx, len1
    int 80h

    mov eax, 4
    mov ebx, 1
    mov ecx, res
    mov edx, 1
    int 80h

    mov eax, 1
    mov ebx, 0
    int 80h
